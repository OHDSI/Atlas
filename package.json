    "rimraf": "^2.6.2",
    "terser": "3.17.0"
  },
  "dependencies": {
    "@ohdsi/atlascharts": "1.7.4",
    "@ohdsi/ui-toolbox": "^1.0.3",
    "@ohdsi/visibilityjs": "^2.0.2",
    "ajv": "^6.10.0",
    "bootstrap": "^3.3.7",
    "bootstrap-select": "1.13.6",
